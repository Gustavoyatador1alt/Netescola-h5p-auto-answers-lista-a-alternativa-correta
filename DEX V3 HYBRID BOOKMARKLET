javascript:(function(){function addEruda(){if(window.eruda)return;const s=document.createElement('script');s.src='https://cdn.jsdelivr.net/npm/eruda';s.onload=()=>{eruda.init()};document.body.appendChild(s);}addEruda();const clean=h=>h?.replace(/<[^>]*>/g," ").replace(/&nbsp;/g," ").replace(/\s+/g," ").trim()||"";const tryParse=s=>{try{return typeof s==="string"?JSON.parse(s):s}catch{return null}};const pullParams=o=>o.params&&(o.params.answers||o.params.question)?o.params:o;function createBox(d=document){let b=d.getElementById("h5p-box");if(!b){b=d.createElement("div");b.id="h5p-box";Object.assign(b.style,{position:"fixed",top:"60px",left:"20px",minWidth:"250px",maxWidth:"400px",background:"#2c3e50",color:"white",border:"2px solid #34495e",borderRadius:"8px",padding:"8px 12px",fontSize:"13px",lineHeight:"1.4",zIndex:"999999",boxShadow:"0 2px 8px rgba(0,0,0,0.3)"});d.body.appendChild(b);}return b;}function extractRows(d=document){const r=[];const push=p=>{const q=pullParams(p||{});const question=clean(q.question||q.prompt||q.stem||q.title||"");const a=Array.isArray(q.answers)?q.answers:[];a.forEach((x,i)=>{const c=x.correct===true||x.isCorrect===true||x.correctAnswer===true;r.push({question,idx:i,letter:String.fromCharCode(65+i),text:clean(x.text||x.label||x.content||""),correct:c,type:"H5P.MultiChoice"})})};const HI=d.defaultView?.H5PIntegration;if(HI?.contents){Object.values(HI.contents).forEach(v=>{const lib=String(v.library||"").split(" ")[0];if(lib==="H5P.MultiChoice"){const p=tryParse(v.jsonContent)||v.content||tryParse(v.params)||null;push(p);}});}const H5P=d.defaultView?.H5P;if(H5P&&Array.isArray(H5P.instances)){H5P.instances.forEach(inst=>{const lib=inst.libraryInfo?.machineName||inst.library?.machineName||inst?.constructor?.name;if(["H5P.MultiChoice","H5P.SingleChoice"].includes(lib)){const p=inst.options||inst.params||inst.contentData?.params||{};push(p);}});}d.querySelectorAll("li.h5p-sc-alternative").forEach((li,i)=>{const c=li.classList.contains("h5p-sc-is-correct");r.push({question:"SingleChoice (DOM)",idx:i,letter:String.fromCharCode(65+i),text:clean(li.innerText||""),correct:c,type:"H5P.SingleChoice"});});return r;}function autoAnswer(r,d=document){const els=d.querySelectorAll(".h5p-sc-alternative.h5p-sc-is-correct");if(els.length>0){els.forEach(el=>el.dispatchEvent(new MouseEvent("click",{bubbles:!0,cancelable:!0})))}const btn=d.querySelector(".h5p-question-check-answer,.h5p-multi-choice-submit,.h5p-sc-next");if(btn&&!btn.disabled)btn.click();}function autoClickNext(d=document){const n=Array.from(d.querySelectorAll("button.nui-button.nui-button-medium.nui-button-curved.nui-button-solid.nui-button-primary")).find(b=>!b.innerText.toLowerCase().includes("avaliar")&&!b.innerText.toLowerCase().includes("experi√™ncia"));if(n&&!n.disabled){console.log("‚ñ∂%EF%B8%8F Avan√ßar");n.click();}}function clickInitialAdvance(d=document){const b=Array.from(d.querySelectorAll("button.nui-button.nui-button-medium.nui-button-curved.nui-button-solid.nui-button-primary")).find(b=>!b.innerText.toLowerCase().includes("avaliar")&&!b.innerText.toLowerCase().includes("experi√™ncia"));if(b&&!b.disabled){console.log("‚ñ∂%EF%B8%8F Avan√ßar inicial");b.click();return!0;}return!1;}function refresh(d=document){if(clickInitialAdvance(d))return;const r=extractRows(d);const box=createBox(d);if(!r.length){box.innerHTML="<b>‚úÖ H5P:</b><br><i>Nenhuma quest√£o encontrada.</i>";return;}autoAnswer(r,d);setTimeout(()=>autoClickNext(d),1500);r.sort((a,b)=>['A','B','C','D'].indexOf(a.letter)-['A','B','C','D'].indexOf(b.letter));let html="<b>‚úÖ H5P Detectado:</b><br><table style='width:100%;font-size:12px;'>";r.forEach(x=>{html+=%60<tr><td>${x.correct?"‚úÖ":"‚ùå"}</td><td>${x.letter})</td><td>${x.text}</td><td style="opacity:0.6;">(${x.type})</td></tr>%60});html+="</table>";box.innerHTML=html;}function scanIframe(){document.querySelectorAll("iframe").forEach(f=>{try{const d=f.contentDocument;if(!d)return;refresh(d);d.addEventListener("DOMContentLoaded",()=>refresh(d));console.log("üîÑ Iframe (same-origin)");}catch(e){console.warn("‚ö†%EF%B8%8F Iframe bloqueado por CORS:",e);}});}function init(){console.log("üß† DEX V3 ativo em:",location.href);refresh(document);setInterval(()=>refresh(document),1000);setInterval(scanIframe,1500);}init();})();
